npc-response-ambivalent_you =
    .a0 = Vypadáš v pohodě.
    .a1 = Po pravdě nemám žádný pocit.
npc-response-dislike_you =
    .a0 = Moc tě nemusím.
    .a1 = Nezdáš se moc milý.
npc-speech-reject_rival =
    .a0 = Nejsi tu vítán.
    .a1 = Promiň, nebudu s tebou mluvit.
    .a2 = Měl bys odejít.
npc-speech-pilot-takeoff =
    .a0 = Vzhůru do { $dst }!
    .a1 = Jdeme na to!
    .a2 = Příští zastávka: { $dst }!
    .a3 = Sbohem, { $src }!
npc-speech-pilot-landed =
    .a0 = Další hladké přistání.
    .a1 = S prstem v nose...
    .a2 = Skvělá práce.
    .a3 = Tady vystupuješ?
npc-speech-pilot-announce_next =
    .a0 = Naskoč, jestli míříš { $dir } do { $dst }!
    .a1 = Brzy vyrážíme do { $dst }!
    .a2 = Cestuješ na { $dir }? Naskoč!
npc-speech-pilot-announce_hold =
    .a0 = Čekáme, než bude volný dok.
    .a1 = Musíme počkat, než se nám uvolní místo v doku.
    .a2 = Kvůli dopravě v doku si musíme udělat přestávku.
npc-speech-pilot-continue_hold =
    .a0 = Musíme ještě počkat!
    .a1 = Je tu spousta dopravy, ještě musíme počkat!
    .a2 = Stále čekáme!
    .a3 = Jiná loď nakládá, ještě budeme chvíli čekat.
npc-speech-merchant_busy_trading =
    .a0 = Počkej prosím, nemůžu se roztrhat.
    .a1 = Chvilku, jen to tu dokončím.
    .a2 = Mám práci, stav se později.
npc-speech-merchant_busy_combat =
    .a0 = Vydrž, něco tu mám!
    .a1 = Teď nemůžu obchodovat, nejdřív se zbavím těch nepřátel!
    .a2 = Uhni, teď bojuju o život!
npc-question-directions =
    .a0 = Kam bys rád vyrazil?
    .a1 = Co hledáš?
npc-response-directions = Vyznačil jsem ti to na mapě. Hodně štěstí!
npc-response-doesnt_exist = Tuhle věc tady nemáme.
